<p style="color: green"><%= notice %></p>

<% content_for :title, "Events" %>

<h1>Events</h1>

<div id="events">
  <% @events.each do |event| %>
    <ul>
      <li>
        <%= link_to(event.name, event) %> - <%= event.datetime %>
      </li>
    </ul>
  <% end %>
</div>

<%= link_to "New event", new_event_path %>
